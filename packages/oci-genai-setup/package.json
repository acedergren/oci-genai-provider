{
  "name": "@acedergren/oci-genai-setup",
  "version": "0.2.0",
  "description": "Setup wizard for OCI GenAI provider with AI SDK, OpenCode, and Claude Code support",
  "engines": {
    "bun": ">=1.0.0",
    "node": ">=18.0.0"
  },
  "bin": {
    "oci-genai-setup": "./dist/cli.js"
  },
  "main": "./dist/cli.js",
  "type": "module",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "bun build src/cli.ts --outdir dist --target bun --minify",
    "dev": "bun --watch src/cli.ts",
    "lint": "eslint src",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@acedergren/oci-genai-provider": "workspace:*",
    "chalk": "^5.3.0",
    "commander": "^12.0.0",
    "open": "^10.0.0",
    "ora": "^8.0.0",
    "prompts": "^2.4.2"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/prompts": "^2.4.9",
    "typescript": "^5.7.3"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://npm.pkg.github.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/acedergren/oci-genai-provider.git",
    "directory": "packages/oci-genai-setup"
  },
  "author": "Alexander Cedergren <alexander.cedergren@oracle.com>",
  "license": "MIT",
  "keywords": [
    "oci",
    "oracle-cloud",
    "genai",
    "ai-sdk",
    "vercel-ai",
    "opencode",
    "claude-code",
    "setup",
    "cli"
  ]
