      "name": "Chat API",
      "item": [
        {
          "name": "GENERIC Format - Basic Chat",
          "event": [
            {
              "listen": "test",
              "script": {
                "exec": [
                  "// Verify response structure matches SDK expectations",
                  "pm.test('Response has chatResult', function() {",
                  "    const jsonData = pm.response.json();",
                  "    pm.expect(jsonData).to.have.property('chatResult');",
                  "});",
                  "",
                  "pm.test('ChatResult has chatResponse', function() {",
                  "    const jsonData = pm.response.json();",
                  "    pm.expect(jsonData.chatResult).to.have.property('chatResponse');",
                  "});",
                  "",
                  "pm.test('ChatResponse has choices array', function() {",
                  "    const jsonData = pm.response.json();",
                  "    const chatResponse = jsonData.chatResult.chatResponse;",
                  "    pm.expect(chatResponse).to.have.property('choices');",
                  "    pm.expect(chatResponse.choices).to.be.an('array');",
                  "});",
                  "",
                  "pm.test('Choice has message with content', function() {",
                  "    const jsonData = pm.response.json();",
                  "    const choice = jsonData.chatResult.chatResponse.choices[0];",
                  "    pm.expect(choice).to.have.property('message');",
                  "    pm.expect(choice.message).to.have.property('content');",
                  "});",
                  "",
                  "pm.test('Choice has finishReason', function() {",
                  "    const jsonData = pm.response.json();",
                  "    const choice = jsonData.chatResult.chatResponse.choices[0];",
                  "    pm.expect(choice).to.have.property('finishReason');",
                  "});",
                  "",
                  "pm.test('ChatResponse has usage', function() {",
                  "    const jsonData = pm.response.json();",
                  "    const chatResponse = jsonData.chatResult.chatResponse;",
                  "    pm.expect(chatResponse).to.have.property('usage');",
                  "    pm.expect(chatResponse.usage).to.have.property('promptTokens');",
                  "    pm.expect(chatResponse.usage).to.have.property('completionTokens');",
                  "});",
                  "",
                  "// Store response for SDK comparison",
                  "pm.collectionVariables.set('lastGenericResponse', JSON.stringify(pm.response.json()));"
                ],
                "type": "text/javascript"
              }
            }
          ],
