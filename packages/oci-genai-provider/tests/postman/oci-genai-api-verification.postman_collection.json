    {
      "name": "SDK Request Structure Validation",
      "item": [
        {
          "name": "Validate SDK Message Format",
          "event": [
            {
              "listen": "prerequest",
              "script": {
                "exec": [
                  "// This represents what the SDK's convertToOCIMessages produces",
                  "const sdkOutput = {",
                  "    role: 'USER',",
                  "    content: [{ type: 'TEXT', text: 'Hello' }]",
                  "};",
                  "",
                  "// Expected OCI API format",
                  "const expectedFormat = {",
                  "    role: 'USER',  // Must be uppercase",
                  "    content: [     // Must be array",
                  "        {",
                  "            type: 'TEXT',  // Must be uppercase",
                  "            text: 'Hello'  // Text content",
                  "        }",
                  "    ]",
                  "};",
                  "",
                  "// Validate SDK output matches expected format",
                  "console.log('SDK produces:', JSON.stringify(sdkOutput));",
                  "console.log('API expects:', JSON.stringify(expectedFormat));",
                  "",
                  "pm.test('SDK role format is correct', function() {",
                  "    pm.expect(sdkOutput.role).to.equal('USER');",
                  "});",
                  "",
                  "pm.test('SDK content is array', function() {",
                  "    pm.expect(sdkOutput.content).to.be.an('array');",
                  "});",
                  "",
                  "pm.test('SDK content type is uppercase', function() {",
                  "    pm.expect(sdkOutput.content[0].type).to.equal('TEXT');",
                  "});"
                ],
                "type": "text/javascript"
              }
            },
            {
              "listen": "test",
              "script": {
                "exec": [
                  "pm.test('Request accepted by API', function() {",
                  "    pm.response.to.have.status(200);",
                  "});"
                ],
                "type": "text/javascript"
              }
            }
          ],
